void practiceHomeRun()
{
    repeat(9) {
        moveForward();
        
    }
    pickBeeper();
    turnLeft();
    repeat(9) {
        moveForward();
    }
    pickBeeper();
    turnLeft();
    repeat(9) {
        moveForward();
    }
    pickBeeper();
    turnLeft();
    repeat(9) {
        moveForward();
        
    }
    pickBeeper();
}
